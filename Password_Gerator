import random
str1="abcdefghijklmnopqrstuvwxyz"
str2="1234567890"
str3="!@#$&*?@|#"
comb_str=str1+str2+str3

user_input=int(input("Enter your length of the password "))
password =""
for i in range(user_input):
    password=password+random.choice(comb_str)
print(f"your strong password is {password}")

